{
  "name": "magic-ui-portfolio-project",
  "version": "1.0.0",
  "description": "Magic UI component portfolio with sitemap generation tools",
  "main": "index.js",
  "scripts": {
    "generate": "node scripts/generate-sitemaps.js",
    "generate:components": "node scripts/generate-sitemaps.js --components",
    "generate:templates": "node scripts/generate-sitemaps.js --templates",
    "generate:all": "node scripts/generate-sitemaps.js --all",
    "download": "node scripts/download-markdowns.js",
    "download:components": "node scripts/download-markdowns.js",
    "portfolio:install": "cd portfolio && npm install",
    "portfolio:dev": "cd portfolio && npm run dev",
    "portfolio:build": "cd portfolio && npm run build",
    "portfolio:start": "cd portfolio && npm start",
    "deploy" :"gh-pages -d portfolio/out"
  },
  "dependencies": {
    "xmlbuilder2": "^3.1.1"
  },
  "devDependencies": {
    "gh-pages": "^6.3.0",
    "prettier": "^3.0.0"
  },
  "keywords": [
    "portfolio",
    "magic-ui",
    "components",
    "sitemap",
    "seo"
  ],
  "author": "",
  "license": "MIT",
  "workspaces": [
    "portfolio"
  ]
}
